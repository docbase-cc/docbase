generator client {
    provider = "prisma-client-js"
    output = "../src/client"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Plugin {
    name   String @id
    config Json
}

model Ext {
    ext        String @id
    pluginName String
}

model KnowledgeBase {
    id   String @id @default(uuid())
    name String
}
